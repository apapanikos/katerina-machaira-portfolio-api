        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "UI",
        "UX",
        "GRAPHIC_DESIGN"
      ]
    },
    "rich_content": {
      "type": "richtext"
    }
  }
}
