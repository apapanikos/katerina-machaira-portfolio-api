{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "author": {
      "model": "author",
      "via": "projects"
    },
    "title": {
      "type": "string"
    },
    "summary": {
      "type": "richtext"
    },
